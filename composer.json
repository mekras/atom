{
    "name": "mekras/atom",
    "description": "Atom Protocol support library",
    "license": "MIT",
    "keywords": [
        "RFC4287",
        "atom"
    ],
    "authors": [
        {
            "name": "Михаил Красильников",
            "email": "m.krasilnikov@yandex.ru"
        }
    ],
    "require": {
        "php": ">=5.6",
        "mekras/class-helpers": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0.9",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Mekras\\Atom\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mekras\\Atom\\Tests\\": "tests/"
        }
    }
}
